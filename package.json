  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/register": "^7.24.6",
    "assert": "^2.1.0",
    "c8": "^10.1.2",
    "chai": "^5.1.1",
